assert (1)._class() == Integer
assert (1.0)._class() == Double
assert "Str"._class() == String
assert true._class() == Bool
assert (1)._class()._super == Number
assert (1.0)._class()._super == Number

var a = 55
assert a._class().isSubclassOf(Number)
assert Number.isSuperclassOf(a._class())
assert !a._class().isSuperclassOf(Double)
assert !a._class().isSubclassOf(Double)

assert Number.name == "Number"
assert Integer.name == "Integer"
assert String.name == "String"
assert Double.name == "Double"
assert Bool.name == "Bool"
