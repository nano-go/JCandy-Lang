
/* basic operation */

var a1 = 50
var a2 = 100
var a3 = 150
var sum = a1 + a2 + a3

assert sum == 300
assert a1 != a2
assert (2 * (a2 + a1)) == 300
assert ((a2 + a1) / 2) == 75
assert (1 * 2 * 3 + a1) == 56
assert (1 * (2 + 1) / 3 - 1 - a1) == -a1
assert (0 / 100) == 0
assert 0.0 / 100 == 0
assert 5 % 10 == 5
assert 5.0 % 10 == 5
assert 3.5 % 2 == 1.5