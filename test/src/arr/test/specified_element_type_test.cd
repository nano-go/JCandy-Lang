
var a = []

class E1 {
	init(value) {
		this.value = value
	}
	
	_str() {
		return "e" + this.value
	}
	
	_equals(obj) {
		if (obj is E1) {
			return this.value == obj.value
		}
		return false
	}
}

class E2: E1 {
	_str() {
		return "ee" + this.value
	}
}

var a = []
a.append(E1(0))
a.append(E2(2))

println(a)
assert a._str() == "[e0, ee2]"
assert a == [E2(0), E1(2)]
