fun lable_test1() {
	outter: while (true) {
		var i
		outter1: for (i in range(0, 15)) {
			for (j in range(0, 15)) {
				if (j == 14) {
					break outter1
				}
			}
			assert i == 0
		}
		break
	}
}

fun lable_test2() {
	var total = 0
	lable:
		for (i in range(0, 10)) {
			for (j in range(0, 10)) {
				if (i * 10 + j > 29) {
					break lable;
				}
				total += i*j
			}
		}
	assert total == 135
}

fun lable_test3() {
	var total = 0
	var i = 0
	lable:
		while (i < 10) {
			var j = 0
			while (j < 10) {
				if (i * 10 + j > 29) {
					break lable;
				}
				total += i*j
				j += 1
			}
			i += 1
		}
	assert total == 135
}


lable_test1()
lable_test2()
lable_test3()