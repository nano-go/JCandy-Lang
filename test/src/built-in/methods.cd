
fun numberMethodTest() {
	var i = 5
	var d = 5.5
	assert d.intVal() == i
	assert d.doubleVal() == d
	assert i.intVal() == i
	assert i.doubleVal() == 5.0
}

fun strMethodTest() {

	/**
	 * Split Test.
	 */
	var str = "a\nb\n" + "c"*15
	assert str.splitlines() == ["a", "b", "c"*15]
	str += "\n"
	assert str.splitlines() == ["a", "b", "c"*15]
	
	assert "".splitlines() == []
	assert "a\n\n".splitlines() == ["a", ""]
	
	var str = "&abcd&efg&hijk%lmn"
	assert str.split("\\w+") == ["&", "&", "&", "%"]
	
	/**
	 * Replace Test.
	 */
	var str = "abcdefg&&&&ssr"
	assert str.replaceFirst("\\w+", "good") == "good&&&&ssr"
	assert str.replaceAll("\\w+", "r") == "r&&&&r"
	
	/**
	 * Substr Test.
	 */
	var str = "abcdefg"
	assert str.substr(0, str.length()) == str
	assert str.substr(0, 1) == "a"
	assert str.substr(1, 3) == "bc"
	assert str.substr(0, 0) == "" && str.substr(5, 2) == ""
	
	/**
	 * Join Test.
	 */
	 assert "%%".join([1, 2, 3, 4]) == "1%%2%%3%%4"
	 assert "%%".join([1]) == "1"
	 assert "%%".join([1, 2]) == "1%%2"
	 assert "%%".join([]) == ""
}

numberMethodTest()
strMethodTest()