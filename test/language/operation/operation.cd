
/* basic operation */

var a1 = 50
var a2 = 100
var a3 = 150
var sum = a1 + a2 + a3


assert sum == 300
assert a1 != a2
assert (2 * (a2 + a1)) == 300
assert ((a2 + a1) / 2) == 75
assert (1 * 2 * 3 + a1) == 56
assert (1 * (2 + 1) / 3 - 1 - a1) == -a1

assert a1 % 5 == 0
assert a2 % 7 == 100 % 7

var a1 = null || a1
assert a1 == 50